{
    "name": "mochi-css",
    "packageManager": "yarn@4.10.3",
    "repository": "git@github.com:Niikelion/mochi-css.git",
    "workspaces": [
        "packages/*",
        "examples/*",
        "!**/dist"
    ],
    "scripts": {
        "build": "turbo build",
        "build.examples": "turbo build-example",
        "build.all": "yarn build && yarn build.examples",
        "test": "turbo test",
        "coverage": "turbo coverage",
        "dev": "turbo dev",
        "format": "turbo format",
        "lint": "turbo lint"
    },
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.18.2",
        "@changesets/cli": "^2.29.7",
        "tsdown": "^0.15.7",
        "turbo": "^2.5.8",
        "typescript": "^5.9.3"
    }
}
