{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [ "^build" ],
            "outputs": [ "dist/**" ]
        },
        "build-example": {
            "dependsOn": [ "^build-example" ],
            "outputs": [ "dist/**" ]
        },
        "test": {
            "dependsOn": [ "^build" ]
        },
        "coverage": {
            "dependsOn": [ "^build" ]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}
